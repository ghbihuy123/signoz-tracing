### MUST SET, map to .env.{ENVIRONMENT}
ENVIRONMENT=dev

#### RabbitMQ config
RABBITMQ_DEFAULT_USER=rabbitmq
RABBITMQ_DEFAULT_PASS=password123
RABBITMQ_DEFAULT_VHOST=computer

#### Celery config
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=${RABBITMQ_DEFAULT_USER}
RABBITMQ_PWD=${RABBITMQ_DEFAULT_PASS}
RABBITMQ_VHOST=computer

# ### Fastapi config
CELERY_BROKER_URL=amqp://${RABBITMQ_DEFAULT_USER}:${RABBITMQ_DEFAULT_PASS}@${RABBITMQ_HOST}:${RABBITMQ_PORT}/${RABBITMQ_DEFAULT_VHOST}
OTEL_SERVICE_NAME=computer-api
OTEL_DEPLOYMENT_ENVIRONMENT=dev
OTEL_MODE=grpc
OTEL_GRPC_ENDPOINT=http://signoz-otel-collector:4317
OTEL_HTTP_ENDPOINT=http://signoz-otel-collector:4318
OTEL_LOGS_SHIP_LEVEL=INFO